{
    "templates": {
        "shop": {
            "layout": "master",
            "components": {
                "hero": {
                    "type": "hero",
                    "properties": {
                        "title": "Welcome to {{ shop.name }}!",
                        "subtitle": "{{ shop.description }}",
                        "cta": null,
                        "cta_text": null,
                        "show_searchbar": true,
                        "show_feedback_rating": true,
                        "show_products_sold": true,
                        "show_total_customers": true,
                        "alignment": "center",
                        "height": "medium",
                        "background_image": "DEFAULT",
                        "side_image": null
